{
    "name": "drmonkeyninja/cakephp-tmdb",
    "description": "CakePHP TMDB API integration",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "tmdb"],
    "homepage": "https://github.com/drmonkeyninja/cakephp-tmdb",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Carter",
            "role": "Author",
            "homepage": "http://andy-carter.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0",
        "php-tmdb/api": "^2.1",
        "muffin/webservice": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "CakeTmdb\\": "src",
            "CakeTmdb\\Lib\\Tmdb\\Driver\\": "src/Lib/Tmdb"
        }
    }
}
